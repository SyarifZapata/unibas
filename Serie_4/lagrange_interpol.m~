function p = lagrange_interpol(x,y,z)
n = length(x);

p= 0
    
    for j = 1:n
  
        
            L =1;
            for k = 1:n
                if k ~= j
                L = L .* (z(i)-x(k))/(x(v)-x(k));  
                
                end
                
            end
            temp = temp + y(v).*L;
       
        p(i)=temp;
    end
end